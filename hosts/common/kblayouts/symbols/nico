partial alphanumeric_keys
xkb_symbols "basic" {
  include "us(basic)"
  include "level3(ralt_switch)"

  name[Group1]= "English - French (Custom Nico)";

  key <CAPS> { [ Escape ] };

  key <AE01>	{ [ 1, exclam, guillemotleft ] };
  key <AE02>	{ [ 2, at, guillemotright ] };
  key <AE04>	{ [ 4, dollar, EuroSign ] };

  key <AD02>	{ [ w, W, eacute, Eacute ] };
  key <AD03>	{ [ e, E, egrave, Egrave ] };
  key <AD07>	{ [ u, U, ugrave, Ugrave ] };
  key <AD09>	{ [ o, O, oe, OE ] };

  key <AC01>	{ [ a, A, agrave, Agrave ] };
  key <AC03>	{ [ d, D, ecircumflex, Ecircumflex ] };
  key <AC07>	{ [ j, J, ucircumflex, Ucircumflex ] };
  key <AC08>	{ [ k, K, icircumflex, Icircumflex ] };
  key <AC09>	{ [ l, L, ocircumflex, Ocircumflex ] };

  key <AB01>	{ [ z, Z, acircumflex, Acircumflex ] };
  key <AB03>	{ [ c, C, ccedilla, Ccedilla ] };
};
